<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">
    <ListPreference
        android:defaultValue="@string/pref_api_online_value"
        android:title="@string/pref_api_profile_title"
        android:key="@string/pref_api_profile_key"
        android:entries="@array/pref_api_option_labels"
        android:entryValues="@array/pref_api_option_values"
        />
    <EditTextPreference
        android:defaultValue=""
        android:inputType="text"
        android:singleLine="true"
        android:title="@string/pref_token_title"
        android:key="@string/pref_token_key"
        />
</PreferenceScreen>